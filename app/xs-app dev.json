{
  "authenticationMethod": "route",
  "welcomeFile": "/index.html",
  "routes": [

    {
      "source": "^/catalog/(.*)$",
      "target": "/catalog/$1",
      "destination": "forecasting-app-srv",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/resources/(.*)$",
      "target": "/resources/$1",
      "authenticationType": "none",
      "destination": "ui5"
    },
  

    {
      "source": "^/(.*)",
      "authenticationType": "xsuaa",
      "localDir": "launchpad/webapp",
      "cacheControl": "no-cache"
    }
  ]
}