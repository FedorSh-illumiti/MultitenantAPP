<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="company1.forecasting.launchpad.controller.launchpad" displayBlock="true"
	xmlns:f="sap.f" xmlns:core="sap.ui.core" id = "launchpad" width = '100%'>
	<VBox id="_IDGenVBox1">
		<f:ShellBar id="sb" homeIcon="{FLP>/homeIcon}" homeIconPressed="onHomeIconPress" navButtonPressed="onNavButtonPressed" >	
            <f:menu >
				<Menu id="_IDGenMenu1">
					<MenuItem text="{i18n>launchpad_ForecastSummary}" icon="sap-icon://vertical-bar-chart-2" press="onMenuPress" id="miForecastSummary"/>
				</Menu>
			</f:menu>
			<f:profile>
				<Avatar id="_IDGenAvatar1" initials="{FLP>/user/initials}" press="onAvatarPressed"></Avatar>
			</f:profile>
		</f:ShellBar>
        <Shell id="_IDGenShell1" width = '100%' appWidthLimited = 'false'>
		<App id="app" isTopLevel = "false" appWidthLimited="false">
		</App>
        </Shell>
	</VBox>
</mvc:View>